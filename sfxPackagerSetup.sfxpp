<scripts>

<script type="initialize">SetProperty(&quot;Add User Guide Shortcut&quot;, &quot;bool&quot;, &quot;yesno&quot;, 1);SetProperty(&quot;Download Latest Source Code&quot;, &quot;bool&quot;, &quot;yesno&quot;, 0);</script>

<script type="preinstall">ShowLicenseAcceptanceDlg();CreateDirectoryTree(&quot;%APPDATA%/Microsoft/Windows/Start Menu/Programs/sfxPackager&quot;);Echo(&quot;Created Start Menu folder...\n&quot;);</script>

<script type="prefile">function CheckDownloadSource(){	if (GetProperty(&quot;Download Latest Source Code&quot;) == 0)	{		Skip();	}}</script>

<script type="postfile">function CreateStartMenuEntry(){	CreateShortcut(&quot;%APPDATA%/Microsoft/Windows/Start Menu/Programs/sfxPackager/sfxPackager.lnk&quot;, FILEPATH, &quot;&quot;, BASEPATH, &quot;sfxPackager (64-bit) install creation utility&quot;, 1, FILEPATH, 0);	Echo(&quot;Created Start Menu shortcut...\n&quot;);}</script>

<script type="postinstall">if ((INSTALLOK == 1) &amp;&amp; (GetProperty(&quot;Add User Guide Shortcut&quot;) == 1)){	Echo(&quot;Adding shortcut to user's guide...\n&quot;);	CreateShortcut(&quot;%APPDATA%/Microsoft/Windows/Start Menu/Programs/sfxPackager/sfxPackager On-line Help.lnk&quot;, &quot;https://docs.google.com/presentation/d/e/2PACX-1vRAVGjiJbSYUrOWB8jEzqG7hMwVbZqvCiAbVmOeL25hoEmN909H-BtGjEawmTMZLta5qHfhGydWDqQd/pub&quot;, &quot;&quot;, &quot;&quot;, &quot;sfxPackager On-Line Help&quot;, 1, &quot;&quot;, 0);}</script>

